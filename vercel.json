{
    "version": 2,
    "env": {
        "PORT": {
            "description": "Porta do Servidor",
            "value": "3000"
        }
    },
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ]
}